    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.1.9",
    "@mojojoejo/vite-plugin-purgecss": "^1.1.0",
    "@tauri-apps/cli": "^1.2.3",
    "@vitejs/plugin-vue": "^4.0.0",
