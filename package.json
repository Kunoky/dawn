{
  "name": "dawn",
  "private": true,
  "description": "vite + vue + pinia + element-ui + axios + vitest",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:stats": " cross-env STATS=true vite build",
    "format": "prettier --write",
    "linter": "eslint",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "axios": "^1.3.3",
    "element-ui": "^2.15.13",
    "oocss": "^0.0.7",
    "pinia": "^2.0.30",
    "vue": "^2.7.0",
    "vue-i18n": "^8.28.2",
    "vue-router": "^3.6.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/test-utils": "^1.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "prettier": "2.8.4",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.58.1",
    "vite": "^4.1.0",
    "vitest": "^0.28.5"
  },
  "lint-staged": {
    "*.{js,jsx,vue,css,md}": [
      "prettier --write",
      "eslint"
    ]
  }
}
